        function withTimeout(promise, ms, errorMsg) {
            return Promise.race([
                promise,
                new Promise((_, reject) => 
                    setTimeout(() => reject(new Error(errorMsg)), ms)
                )
            ]);
        }

        function isFirebaseReady() {
            return typeof window.firebaseReady !== 'undefined' && window.firebaseReady === true;
        }

        async function saveToFirebase(teamName, data) {
            if (!isFirebaseReady()) {
                console.warn('âš ï¸ Firebase not ready');
                return false;
            }
            
            try {
                console.log(`ğŸ’¾ Saving to Firebase...`);
                
                await withTimeout(
                    window.firebaseSetDoc(
                        window.firebaseDoc(window.firebaseDB, 'teams', teamName),
                        data
                    ),
                    5000,
                    'Firebase save timeout'
                );
                
                console.log('âœ… Saved to Firebase');
                return true;
            } catch (error) {
                console.error('âŒ Firebase save error:', error.message);
                return false;
            }
        }

        async function loadFromFirebase(teamName) {
            if (!isFirebaseReady()) {
                console.warn('âš ï¸ Firebase not ready');
                return null;
            }
            
            try {
                console.log(`ğŸ”¥ Loading from Firebase...`);
                
                const docRef = window.firebaseDoc(window.firebaseDB, 'teams', teamName);
                const docSnap = await withTimeout(
                    window.firebaseGetDoc(docRef),
                    5000,
                    'Firebase load timeout'
                );
                
                if (docSnap.exists()) {
                    console.log('âœ… Loaded from Firebase');
                    return docSnap.data();
                } else {
                    console.log('â„¹ï¸ No Firebase data');
                    return null;
                }
            } catch (error) {
                console.error('âŒ Firebase load error:', error.message);
                return null;
            }
        }

        function getStorageKey(teamCode) {
            return `unlocked_teks_${teamCode}`;
        }

		async function getTeamData(teamCode) {
